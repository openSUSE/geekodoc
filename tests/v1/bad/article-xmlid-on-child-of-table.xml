<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="geekodoc-v1-flat.rnc" type="application/relax-ng-compact-syntax"?>
<article xmlns="http://docbook.org/ns/docbook">
 <title>Test xml:id on an informaltable</title>
 <table xml:id="table">
  <title></title>
  <tgroup cols="2" xml:id="tgroud-id">
   <thead xml:id="thead-id">
    <row>
     <entry>Head A</entry>
     <entry>Head B</entry>
    </row>
   </thead>
   <tfoot xml:id="tfoot-id">
    <row>
     <entry>Foot A</entry>
     <entry>Foot B</entry>
    </row>
   </tfoot>
   <tbody xml:id="tbody-id">
    <row xml:id="row-id">
     <entry xml:id="entry-id">A</entry>
     <entry>B</entry>
    </row>
   </tbody>
  </tgroup>
 </table>
</article>
